{
  "name": "vite-vue2-ts-starter",
  "private": true,
  "version": "0.0.1",
  "description": "Vue2 SRR TypeScript Startar project for Vite.",
  "homepage": "https://github.com/fallemand/vue2-ssr-vite-ts",
  "repository": {
    "type": "git",
    "url": "git@github.com:fallemand/vue2-ssr-vite-ts.git"
  },
  "bugs": {
    "url": "https://github.com/fallemand/vue2-ssr-vite-ts/issues"
  },
  "engines": {
    "node": ">=16"
  },
  "license": "MIT",
  "scripts": {
    "start:dev": "NODE_ENV=development node server",
    "start:prod": "NODE_ENV=production node server",
    "build": "npm run build:client && npm run build:server",
    "build:client": "vite build --outDir dist/client --ssrManifest",
    "build:server": "vite build --outDir dist/server --ssr src/entry-server.js"
  },
  "dependencies": {
    "express": "^4.17.2",
    "serve-static": "^1.14.2",
    "vue": "^2.6.14",
    "vue-class-component": "^7.2.6",
    "vue-property-decorator": "^9.1.2",
    "vue-router": "^3.5.3",
    "vue-server-renderer": "^2.6.14",
    "vuex": "^3.6.2"
  },
  "devDependencies": {
    "@types/node": "^17.0.17",
    "@vue/compiler-sfc": "^3.2.30",
    "typescript": "^4.5.5",
    "vite": "^2.8.1",
    "vite-plugin-vue2": "^1.9.3",
    "vue-template-compiler": "^2.6.14",
    "vue-tsc": "^0.31.2"
  }
}
